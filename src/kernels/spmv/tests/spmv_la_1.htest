#include <kernels/spmv/spmv_la_1.cu>
#ifndef SPMV_LA_1_DRIVER
#define SPMV_LA_1_DRIVER(PRELOAD, INCLUDE_PRELOAD_ARITH, CHUNK_PARTS) spmv_la_v1_ ## PRELOAD ## _ ## INCLUDE_PRELOAD_ARITH ## _ ## CHUNK_PARTS ## _driver
#endif

#ifndef SPMV_LA_1
#define INDIRECT_COPY(PRELOAD, INCLUDE_PRELOAD_ARITH, CHUNK_PARTS) \
	{ \
		auto driver = MicrobenchmarkDriver<SpmvKernelLAv1<vt, it, PRELOAD, INCLUDE_PRELOAD_ARITH, CHUNK_PARTS>>
			(N, bs_vec, output_dir+ XSTRINGIFY( SPMV_LA_1_DRIVER(PRELOAD, INCLUDE_PRELOAD_ARITH, CHUNK_PARTS) ) ".csv", &dev_ctx, span_occupancies); \
		if (!driver.check_then_run_kernels()) {return -1;}  \
		total_runs += driver.get_total_runs(); \
	}
#endif
